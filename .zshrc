    eval "$__conda_setup"
else
    if [ -f "/Users/Joshua/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/Joshua/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/Joshua/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
